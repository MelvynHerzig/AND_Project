<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/green"
    tools:context=".WhackGameActivity">

    <!-- Holes -->
    <!-- First line of holes -->
    <ImageView
        android:id="@+id/hole_1"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginEnd="100dp"
        android:contentDescription="@string/hole_image"
        android:scaleType="fitXY"
        android:src="@drawable/hole"
        app:layout_constraintBottom_toBottomOf="@id/hole_4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@id/hole_2"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/hole_2"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:contentDescription="@string/hole_image"
        android:scaleType="fitXY"
        android:src="@drawable/hole"
        app:layout_constraintBottom_toBottomOf="@id/hole_5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/hole_3"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="100dp"
        android:contentDescription="@string/hole_image"
        android:scaleType="fitXY"
        android:src="@drawable/hole"
        app:layout_constraintBottom_toBottomOf="@id/hole_6"
        app:layout_constraintLeft_toLeftOf="@id/hole_2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!-- Second line of holes -->
    <ImageView
        android:id="@+id/hole_4"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginEnd="100dp"
        android:contentDescription="@string/hole_image"
        android:scaleType="fitXY"
        android:src="@drawable/hole"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@id/hole_5"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/hole_5"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:contentDescription="@string/hole_image"
        android:scaleType="fitXY"
        android:src="@drawable/hole"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/hole_6"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="100dp"
        android:contentDescription="@string/hole_image"
        android:scaleType="fitXY"
        android:src="@drawable/hole"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/hole_5"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!-- Last line of holes -->
    <ImageView
        android:id="@+id/hole_7"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginEnd="100dp"
        android:contentDescription="@string/hole_image"
        android:scaleType="fitXY"
        android:src="@drawable/hole"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@id/hole_8"
        app:layout_constraintTop_toBottomOf="@id/hole_4" />

    <ImageView
        android:id="@+id/hole_8"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:contentDescription="@string/hole_image"
        android:scaleType="fitXY"
        android:src="@drawable/hole"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/hole_5" />

    <ImageView
        android:id="@+id/hole_9"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="100dp"
        android:contentDescription="@string/hole_image"
        android:scaleType="fitXY"
        android:src="@drawable/hole"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/hole_8"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/hole_6" />

    <!-- Quokka -->
    <!-- First line of quokka -->
    <ImageView
        android:id="@+id/quokka_1"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginEnd="100dp"
        android:contentDescription="@string/quokka_image"
        android:scaleType="fitXY"
        android:src="@drawable/quokka"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@id/hole_4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@id/hole_2"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/quokka_2"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:contentDescription="@string/quokka_image"
        android:scaleType="fitXY"
        android:src="@drawable/quokka"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@id/hole_5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/quokka_3"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="100dp"
        android:contentDescription="@string/quokka_image"
        android:scaleType="fitXY"
        android:src="@drawable/quokka"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@id/hole_6"
        app:layout_constraintLeft_toLeftOf="@id/hole_2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!-- Second line of quokka -->
    <ImageView
        android:id="@+id/quokka_4"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginEnd="100dp"
        android:contentDescription="@string/quokka_image"
        android:scaleType="fitXY"
        android:src="@drawable/quokka"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@id/hole_5"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/quokka_5"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:contentDescription="@string/quokka_image"
        android:scaleType="fitXY"
        android:src="@drawable/quokka"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/quokka_6"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="100dp"
        android:contentDescription="@string/quokka_image"
        android:scaleType="fitXY"
        android:src="@drawable/quokka"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/hole_5"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!-- Last line of quokka -->
    <ImageView
        android:id="@+id/quokka_7"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginEnd="100dp"
        android:contentDescription="@string/quokka_image"
        android:scaleType="fitXY"
        android:src="@drawable/quokka"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@id/hole_8"
        app:layout_constraintTop_toBottomOf="@id/hole_4" />

    <ImageView
        android:id="@+id/quokka_8"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:contentDescription="@string/quokka_image"
        android:scaleType="fitXY"
        android:src="@drawable/quokka"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/hole_5" />

    <ImageView
        android:id="@+id/quokka_9"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="50dp"
        android:contentDescription="@string/quokka_image"
        android:scaleType="fitXY"
        android:src="@drawable/quokka"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/hole_8"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/hole_6" />

</androidx.constraintlayout.widget.ConstraintLayout>